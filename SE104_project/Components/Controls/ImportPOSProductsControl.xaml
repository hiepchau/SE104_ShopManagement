<UserControl x:Class="SE104_OnlineShopManagement.Components.Controls.ImportPOSProductsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:uc="clr-namespace:SE104_OnlineShopManagement.Components.Controls"
             
             xmlns:local="clr-namespace:SE104_OnlineShopManagement.Components.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="40">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="266*"/>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition Width="107*"/>
            <ColumnDefinition Width="40"/>
        </Grid.ColumnDefinitions>
        <TextBlock 
            Text="STT" 
            Grid.Column="0"
            Style="{StaticResource Text}"/>
        <TextBlock 
            Grid.Column="1"
            Text="Tên sản phẩm" 
            Style="{StaticResource Text}"
            HorizontalAlignment="Left"                                
            Margin="20,0,0,0"/>
        <uc:NumericSnipperControl             
           x:Name="nmsQuantity"
            Width="80" 
            Height="32"
            Margin="0" 
            Grid.Column="2" >
        </uc:NumericSnipperControl>

            
            
            <TextBlock 
            Grid.Column="3" 
            Text="Giá nhập" 
            Style="{StaticResource Text}"/>

    </Grid>
</UserControl>
